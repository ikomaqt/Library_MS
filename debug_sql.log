SELECT b.id, b.BookName, b.RegDate, 
                   b.bookImage AS BookImage, a.AuthorName, c.CategoryName 
            FROM tblbooks b
            JOIN tblauthors a ON b.AuthorId = a.id
            JOIN tblcategory c ON b.CatId = c.id
            WHERE 1=1 ORDER BY b.BookName ASC
